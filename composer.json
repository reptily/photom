{
    "name": "photom/framework",
    "description": "micro open framework",
    "license": "MIT",
    "version":"1.2.0",
    "authors": [
        {
            "name": "Reptily",
            "email": "reptily@go2ride.ru"
        }
    ],
    "keywords": [
        "framework",
        "openWeb",
        "reptily",
        "photom",
        "php"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4":{
            "Ph\\":"src"
        }
